  borderless = True
  handle_size = -3
  tab_position = bottom
[keybindings]
  close_term = <Alt>q
  go_down = <Alt>j
  go_left = <Alt>h
  go_right = <Alt>l
  go_up = <Alt>k
  hide_window = F12
  layout_launcher = <Shift><Alt>l
  split_horiz = <Alt>o
  split_vert = <Alt>i
  toggle_zoom = <Alt>m
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[default]]
    background_darkness = 0.9
    background_image = None
    background_type = transparent
    cursor_blink = False
